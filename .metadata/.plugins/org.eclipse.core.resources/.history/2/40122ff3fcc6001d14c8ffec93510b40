package file;

public class NcFile {
	private static String NcFile;
	
	public static void setNcFile(String addedLines) {
		if (WireID.getWireID().contains("km")) {
			addedLines = addedLines.substring(37);
		} 
		NcFile = addedLines.substring(addedLines.indexOf('-') + 1, addedLines.indexOf(".NC") + 3);
		
		System.out.println(NcFile);
	}
}
